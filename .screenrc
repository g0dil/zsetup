attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E38;5;%dm'
defbce "on"
caption always "%{= wk} %-w%{= KW} [%n %t] %{-}%+w %= | @%H | %l | %Y-%m-%d %c "
escape ^zz
shell /usr/bin/zsh
setenv SSH_AUTH_SOCK $HOME/.ssh/ssh_auth_sock
bindkey ^[[1;2D prev
bindkey ^[[1;2C next
defscrollback 8192
